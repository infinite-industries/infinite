    "id":"85f15fb6-09be-48d2-8970-04f3bb1804ad",
    "title":"Crossing Mulholland + Q&A with Tom Thurman and Guests",
    "type" :"calendar_event",
    "slug":"crossing-mulholland-+-q&a-with-tom-thurman-and-guests",
    "when_date":"Friday, July 13th, 2018",
    "when_time":"7:00pm - 9:00pm",
