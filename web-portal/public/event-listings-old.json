         "website": "none",
         "image":"https://s3-us-west-2.amazonaws.com/infinite-industries-event-images/uploads/45c103e0-2d49-11e8-91ff-ff041baf9fad.jpg",
         "social_image":"none",
         "venues":["Lyric Theatre & Cultural Arts Center"],
         "admission_fee": "$25.00",
         "address": "300 E Third St, Lexington, KY 40508",
