{
  "name": "my-glitch-app",
  "version": "0.0.1",
  "description": "Prototype art event portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "watchify": "watchify -vd -p browserify -e ./src/main.js -o ./public/js/dist.js",
    "production-build": "NODE_ENV=production browserify -g envify -e ./src/main.js | uglifyjs -c -m > ./public/js/dist.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "dependencies": {
    "addtocalendar": "^1.5.3",
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "moment": "^2.18.1",
    "nunjucks": "^3.0.1",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1",
    "vuetify": "^0.15.7"
  },
  "devDependencies": {
    "watchify": "^3.9.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "vueify": "^9.4.1",
    "envify": "^4.1.0",
    "uglify": "^0.1.5"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/welcome-project"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "art",
    "culture",
    "open source"
  ]
}
