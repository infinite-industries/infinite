{
  "name": "infinite-api",
  "version": "0.0.1",
  "description": "database and for art work/event management and presentation",
  "author": {
    "name": "Infinite Crew",
    "url": "https://infinite.industries/who-we-are"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run copy-constants",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "sequelize-cli db:migrate && nest start",
    "start:dev": "sequelize-cli db:migrate && npm run copy-constants && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "copy-constants": "copyfiles -u src/db_constants.js dist/",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node ./test/clearLogFile.js && nest build && jest --config ./test/jest-e2e.json --runInBand",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:down-one": "sequelize-cli db:migrate:undo",
    "db:migrate:generate": "sequelize-cli migration:generate",
    "db:seed": "sequelize db:seed:all",
    "db:seed:venues": "sequelize db:seed --seed 20190929142748-venues.js"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^9.0.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@types/jsonwebtoken": "^9.0.2",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^9.0.0 ",
    "moment": "~2.29.1",
    "moment-timezone": "~0.5.33",
    "nest-winston": "^1.9.1",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "slack-notify": "^2.0.6 ",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/node-geocoder": "^4.2.0",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "class-validator": "^0.14.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "frisby": "^2.1.3",
    "jest": "29.5.0",
    "jest-cli": "^26.5.2",
    "node-geocoder": "^4.2.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "testcontainers": "^6.0.0",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
