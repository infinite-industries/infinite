{
  "name": "infinite-api",
  "version": "0.0.1",
  "description": "database and for art work/event management and presentation",
  "author": {
    "name": "Infinite Crew",
    "url": "https://infinite.industries/who-we-are"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "nest build && jest --config ./test/jest-e2e.json --runInBand",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:generate": "sequelize-cli migration:generate"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.6.13",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/sequelize": "^0.1.1",
    "@nestjs/swagger": "^4.7.15",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "nest-winston": "^1.4.0",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "slack-notify": "^0.1.7",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "moment": "~2.29.1",
    "moment-timezone": "~0.5.33"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.4",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "class-validator": "^0.12.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "faker": "^5.5.3",
    "frisby": "^2.1.3",
    "jest": "^26.0.1",
    "jest-cli": "^26.5.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "testcontainers": "^6.0.0",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
